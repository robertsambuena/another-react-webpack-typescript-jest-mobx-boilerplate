{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "no-var-requires": false,
    "no-submodule-imports": false,
    "no-console": false,
    "no-implicit-dependencies": [true, "dev"],
    "variable-name": [ true, "check-format", "allow-pascal-case" ]
  },
  "rulesDirectory": []
}
